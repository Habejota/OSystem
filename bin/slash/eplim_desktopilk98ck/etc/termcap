


dumb|80-column dumb tty:\
	:am:\
	:co#80:\
	:bl=^G:cr=^M:do=^J:sf=^J:
unknown|unknown terminal type:\
	:am:gn:\
	:co#80:\
	:bl=^G:cr=^M:do=^J:sf=^J:
lpr|printer|line printer:\
	:bs:hc:os:\
	:co#132:li#66:\
	:bl=^G:cr=^M:do=^J:ff=^L:le=^H:sf=^J:

vanilla|dumb tty:\
	:bs:\
	:bl=^G:cr=^M:do=^J:sf=^J:
ansi+erase:\
	:cd=\E[J:ce=\E[K:cl=\E[H\E[J:
ansi+cup:\
	:cm=\E[%i%d;%dH:ho=\E[H:

